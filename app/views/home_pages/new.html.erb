<h1>HomePages#new</h1>
<%= form_for @lec, url: home_pages_url, method: :post do |f|%>
<p><%=f.label :name %><%=f.text_field :name%></p>
<p><%=f.label :emp_no %><%=f.text_field :emp_no%></p>
<p><%=f.label :mobile_no %><%=f.text_field :mobile_no%></p>
<p><%=f.label :email_id %><%=f.text_field :email_id%></p>
<p><%=f.label :dob %><%=f.text_field :dob%></p>
<p><%=f.label :address %><%=f.text_area :address%></p>
<%=f.submit "submit" %>
<% end %>
